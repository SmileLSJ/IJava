<%@ page contentType="text/html; charset=GBK"%>
<%@ taglib uri="kingfisher" prefix="fk"%>
<html>
<head>
<META http-equiv="X-UA-Compatible" content="IE=EmulateIE7">
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<title></title>
</head>
#fkview("config=${cfgFileName}")
	<body topmargin="0" leftmargin="0" rightmargin="0" bottommargin="0"
		bgcolor="#F1F4FA">
	<table border="0" width="240" id="table1" class="autoform"
		cellpadding="0" cellspacing="0" height="100%">
		<tr>
			<td height="100%"></td>
		</tr>
		<tr>
			<td>
			<fieldset class="autoform_groupbevel" style="width:100%;height:20;">
			<legend class="autoform_grouptitle">起始日期</legend>
			<table width="100%" height="100%" style="TABLE-LAYOUT:fixed">
				<tr>
					<td>#fktexteditor("id=edtSTDT")-#fktexteditor("id=edtEDDT")
					
					</td>
				</tr>
			</table>
			</fieldset>
			</td>
		</tr>
		<tr>
			<td>
			<fieldset class="autoform_groupbevel"
				style="width: 100%; height: 10;"><legend
				class="autoform_grouptitle">其它条件</legend>
			<table>
				<tr>
					<td>日志分类<br />
					#fktexteditor("id=edtLOGKD")<br />
					日志类型<br />
					#fktexteditor("id=edtLOGTP")<br />
					日志名称<br />
					#fktexteditor("id=edtLOGCD")
					</td>
				</tr>
			</table>
			</fieldset>
			</td>
		</tr>
		<tr>
			<td height="5%">
			<table cellspacing="0" cellpadding="3" height="100%"
				style="TABLE-LAYOUT: fixed">
				<tr>
					<td align="right">#fkbutton("id=btnSearch")</td>
				</tr>
			</table>
			</td>
		</tr>
	</table>
	</body>
#end
</html>
