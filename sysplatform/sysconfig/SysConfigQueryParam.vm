	<div>
		<p>配置类型</p>
		<div>#fktexteditor("id=CfgGroup")</div>
	</div>
	<div>
		<p>配置编码</p>
		<div>#fktexteditor("id=CfgCode")</div>
	</div>
	<div>
		<p>配置名称</p>
		<div>#fktexteditor("id=CfgName")</div>
	</div>
</div>
</div>
<div class="searchButton">
<div class="refresh" onclick="window.location.reload();">刷新</div><div class="search" onclick="paramSearch();">查询</div>
