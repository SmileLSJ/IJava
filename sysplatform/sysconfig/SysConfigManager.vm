<html>
<head>
<META http-equiv="X-UA-Compatible" content="IE=EmulateIE7">
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>配置管理</title>
<link rel="stylesheet" type="text/css" href="${applicationRoot}/exhibit/sysref/SystemModule.css"></link>

</head>
#fkview("config=${cfgFileName}")

	<body topmargin="0" leftmargin="0" rightmargin="0" bottommargin="0"
		bgcolor="#F1F4FA">
		<div class="form_module">
			<div>
				<div>
					<div class="l">
						<font color="red">*</font>配置编码
					</div>
					<div class="r">
						#fktexteditor("id=CfgCode")
					</div>
				</div>
				<div>
					<div class="l">
						<font color="red">*</font>配置类型
					</div>
					<div class="r">
						#fktexteditor("id=CfgGroup")
					</div>
				</div>
				<div>
					<div class="l">
						<font color="red">*</font>配置名称
					</div>
					<div class="r">
						#fktexteditor("id=CfgName")
					</div>
				</div>
				<div>
					<div class="l">
						<font color="red">*</font>主配置值
					</div>
					<div class="r">
						#fktexteditor("id=CfgValue")
					</div>
				</div>
				<div>
					<div class="l">次配置值</div>
					<div class="r">
						#fktexteditor("id=CfgSValue")
					</div>
				</div>
				<div class="h50">
					<div class="l">配置说明</div>
					<div class="r">
						#fktexteditor("id=CfgLabel")
					</div>
				</div>
			</div>
		</div>
		<div class="form_button">
	    	#fkbutton("id=btnClose")
	        #fkbutton("id=btnSave")
    	</div>
	</body>
#end

</html>
